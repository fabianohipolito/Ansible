;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	debian.futurama.com.br. root.futurama.com.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	IN	NS	debian.
1 	IN 	PTR	gateway.futuram.com.br.
60	IN	PTR	git.futurama.com.br.
10	IN	PTR	debian.futuram.com.br.

